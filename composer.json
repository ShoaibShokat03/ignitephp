{
  "name": "ignitephp/project",
  "description": "IgnitePHP starter project",
  "type": "project",
  "license": "MIT",
  "authors": [
    {
      "name": "Muhammad Shoaib",
      "email": "shoaibshokat6@gmail.com"
    }
  ],
  "require": {
    "ignitephp/ignitephp": "dev-main",
    "vlucas/phpdotenv": "^5.6"
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "autoload": {
    "psr-4": {
      "App\\": "app/",
      "Config\\": "config/",
      "Ignitephp\\": "src/"
    }
  },
  "scripts": {
    "post-create-project-cmd": [
      "@php -r \"copy('.env.example', '.env');\""
    ]
  }
}
